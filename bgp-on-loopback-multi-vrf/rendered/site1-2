######################################
# 01-Edge-Underlay.j2
######################################
config system settings
  set location-id 10.200.1.2
  set allow-subnet-overlap enable
end
config system global
    set hostname site1-2
end
config system interface
  edit "port1"
    set mode dhcp
    set vrf 32
    set role wan
    set allowaccess ping
  next
end
config system interface
  edit "port2"
    set mode dhcp
    set vrf 32
    set role wan
    set allowaccess ping
  next
end
config system interface
  edit "port4"
    set mode static
    set ip 172.16.0.9/29
    set vrf 32
    set role wan
    set allowaccess ping
  next
end
config system interface
  edit "port5"
    set mode static
    set ip 10.0.2.1/24
    set vrf 1
    set role lan
    set device-identification enable
    set allowaccess ping
  next
end
  config system vdom-link
    edit "vrf1_leak"
    next
  end
  config system interface
    edit "vrf1_leak0"
      set type vdom-link
      set vrf 32
      set vdom 'root'
      set ip 10.200.255.3/31
      set allowaccess ping
    next
    edit "vrf1_leak1"
      set type vdom-link
      set vrf 1
      set vdom 'root'
      set ip 10.200.255.2/31
      set allowaccess ping
    next
  end
  config router static
    edit 10001
      set gateway 10.200.255.3
      set device vrf1_leak1
    next
  end
  config system zone
    edit "pevrf_leak_zone"
      append interface "vrf1_leak0"
    next
    edit "vrfs_leak_zone"
      append interface "vrf1_leak1"
    next
  end
config system interface
  edit "port6"
    set mode static
    set ip 10.0.102.1/24
    set vrf 2
    set role lan
    set device-identification enable
    set allowaccess ping
  next
end
config system interface
  edit "Lo"
    set vdom "root"
    set type loopback
    set vrf 32
    set ip 10.200.1.2/32
    set allowaccess ping
  next
end
config system zone
  edit "lan_zone"
    set interface port5 port6
  next
end
config system dhcp server
  edit 10
    set dns-service default
    set default-gateway 10.0.2.1
    set netmask 255.255.255.0
    set interface port5
    config ip-range
      edit 1
        set start-ip 10.0.2.4
        set end-ip 10.0.2.251
      next
    end
  next
  edit 11
    set dns-service default
    set default-gateway 10.0.102.1
    set netmask 255.255.255.0
    set interface port6
    config ip-range
      edit 1
        set start-ip 10.0.102.4
        set end-ip 10.0.102.251
      next
    end
  next
end

######################################
# 02-Edge-Overlay.j2
######################################
config vpn ipsec phase1-interface
  edit "H1_ISP1"
    set interface "port1"
    set ike-version 2
    set psksecret S3cr3t!
    set keylife 28800
    set peertype any
    set net-device enable
    set proposal aes256gcm-prfsha256
    set idle-timeout enable
    set auto-discovery-receiver enable
    set encapsulation vpn-id-ipip
    set exchange-ip-addr4 10.200.1.2
    set add-route disable
    set network-overlay enable
    set network-id 11
    set remote-gw 100.64.1.1
    set dpd-retrycount 3
    set dpd-retryinterval 5
    set dpd on-idle
  next
end
config vpn ipsec phase2-interface
  edit "H1_ISP1"
    set phase1name "H1_ISP1"
    set proposal aes256gcm
    set keepalive enable
    set keylifeseconds 3600
  next
end
config system interface
  edit "H1_ISP1"
    set vrf 32
  next
end
config vpn ipsec phase1-interface
  edit "H1_ISP2"
    set interface "port2"
    set ike-version 2
    set psksecret S3cr3t!
    set keylife 28800
    set peertype any
    set net-device enable
    set proposal aes256gcm-prfsha256
    set idle-timeout enable
    set auto-discovery-receiver enable
    set encapsulation vpn-id-ipip
    set exchange-ip-addr4 10.200.1.2
    set add-route disable
    set network-overlay enable
    set network-id 12
    set remote-gw 100.64.1.9
    set dpd-retrycount 3
    set dpd-retryinterval 5
    set dpd on-idle
  next
end
config vpn ipsec phase2-interface
  edit "H1_ISP2"
    set phase1name "H1_ISP2"
    set proposal aes256gcm
    set keepalive enable
    set keylifeseconds 3600
  next
end
config system interface
  edit "H1_ISP2"
    set vrf 32
  next
end
