######################################
# 03-Edge-Routing.j2
######################################
config router route-map
  edit "H1_TAG"
    config rule
      edit 1
        set set-tag 1
      next
    end
  next
  edit "H2_TAG"
    config rule
      edit 1
        set set-tag 2
      next
    end
  next
  edit "SLA_OK"
    config rule
      edit 1
        set set-community "65001:99"
      next
    end
  next
end
config router bgp
  set as 65001
  set router-id 10.200.1.2
  set keepalive-timer 15
  set holdtime-timer 45
  set ibgp-multipath enable
  set recursive-next-hop enable
  set tag-resolve-mode merge
  config neighbor
    edit 10.200.1.253
      set soft-reconfiguration enable
      set soft-reconfiguration-vpnv4 enable
      set advertisement-interval 1
      set interface "Lo"
      set update-source "Lo"
      set connect-timer 1
      set remote-as 65001
      set route-map-in "H1_TAG"
      set route-map-in-vpnv4 "H1_TAG"
      set route-map-out-vpnv4-preferable "SLA_OK"
    next
    edit 10.200.1.254
      set soft-reconfiguration enable
      set soft-reconfiguration-vpnv4 enable
      set advertisement-interval 1
      set interface "Lo"
      set update-source "Lo"
      set connect-timer 1
      set remote-as 65001
      set route-map-in "H2_TAG"
      set route-map-in-vpnv4 "H2_TAG"
      set route-map-out-vpnv4-preferable "SLA_OK"
    next
  end
  config network
      edit 1
        set prefix 10.0.2.0/24
      next
      edit 2
        set prefix 10.0.102.0/24
      next
    end
  config vrf
    edit "32"
      set role pe
    next
    edit "1"
      set role "ce"
      set rd "65000:1"
      set import-rt "65000:1"
      set export-rt "65000:1"
    next
    edit "2"
      set role "ce"
      set rd "65000:2"
      set import-rt "65000:2"
      set export-rt "65000:2"
    next
  end
end

######################################
# optional/05-Edge-SDWAN.j2
######################################
config firewall address
  edit "CORP_LAN"
    set subnet 10.0.0.0/8
  next
end
config system sdwan
  set status enable
  config zone
    edit "underlay"
    next
    edit "overlay"
    next
  end
  config members
    edit 1
      set interface "port1"
      set zone "underlay"
      # Best priority, used for Internet access in implicit rule / local-out
      # Gateway is fetched from DHCP
    next
    edit 2
      set interface "port2"
      set zone "underlay"
      # Best priority, used for Internet access in implicit rule / local-out
      # Gateway is fetched from DHCP
    next
    edit 3
      set interface "H1_ISP1"
      set priority 10
      set source 10.200.1.2
      set zone "overlay"
    next
    edit 4
      set interface "H1_ISP2"
      set priority 10
      set source 10.200.1.2
      set zone "overlay"
    next
    edit 5
      set interface "H1_MPLS"
      set priority 10
      set source 10.200.1.2
      set zone "overlay"
    next
    edit 6
      set interface "H2_ISP1"
      set priority 10
      set source 10.200.1.2
      set zone "overlay"
    next
    edit 7
      set interface "H2_ISP2"
      set priority 10
      set source 10.200.1.2
      set zone "overlay"
    next
    edit 8
      set interface "H2_MPLS"
      set priority 10
      set source 10.200.1.2
      set zone "overlay"
    next
  end
  config health-check
    edit "HUB"
      set server 10.200.99.1
      set sla-fail-log-period 10
      set sla-pass-log-period 10
      set embed-measured-health enable
      set members 3 4 5 6 7 8
      config sla
        edit 1
          set link-cost-factor latency
          set latency-threshold 100
        next
      end
    next
    edit "Internet"
      set server "www.fortinet.com"
      set sla-fail-log-period 10
      set sla-pass-log-period 10
      set members 1 2
    next
  end
  config neighbor
    edit 10.200.1.253
      set member 3 4 5
      set health-check "HUB"
      set sla-id 1
      set minimum-sla-meet-members 1
    next
    edit 10.200.1.254
      set member 6 7 8
      set health-check "HUB"
      set sla-id 1
      set minimum-sla-meet-members 1
    next
  end
  config service
    edit 1
      set name "Corporate-H1"
      set mode sla
      set dst "CORP_LAN"
      set src "CORP_LAN"
      set hold-down-time 20
      set tie-break fib-best-match
      config sla
        edit "HUB"
          set id 1
        next
      end
      # Using Hub site1-H1
      set priority-members 3 4 5
    next
    edit 2
      set name "Corporate-H2"
      set mode sla
      set dst "CORP_LAN"
      set src "CORP_LAN"
      set hold-down-time 20
      set tie-break fib-best-match
      config sla
        edit "HUB"
          set id 1
        next
      end
      # Using Hub site1-H2
      set priority-members 6 7 8
    next
    edit 10
      set name "Local-Breakout"
      set mode priority
      set dst "all"
      set health-check Internet
      # Using best underlay (DIA) link
      set priority-members 1 2
    next
  end
end
config router static
  edit 100
    # Default route
    set sdwan-zone "underlay" "overlay"
  next
end

######################################
# optional/06-Edge-Firewall.j2
######################################
config firewall address
  edit "CORP_LAN"
    set subnet 10.0.0.0/8
  next
end
config firewall ippool
  edit "Lo"
    set startip 10.200.1.2
    set endip 10.200.1.2
  next
end
config firewall policy
  edit 1
    set name "Corporate"
    set srcintf lan_zone "overlay"
    set dstintf lan_zone "overlay"
    set srcaddr "CORP_LAN"
    set dstaddr "CORP_LAN"
    set action accept
    set schedule "always"
    set service "ALL"
    set utm-status enable
    set ssl-ssh-profile certificate-inspection
    set application-list "default"
    set logtraffic all
  next
  edit 2
    set name "Internet (to PE)"
    set srcintf lan_zone
    set dstintf vrfs_leak_zone
    set srcaddr "all"
    set dstaddr "all"
    set action accept
    set schedule "always"
    set service "ALL"
    set utm-status enable
    set ssl-ssh-profile certificate-inspection
    set application-list "default"
    set logtraffic all
    set nat enable
  next
  edit 3
    set name "Internet (DIA)"
    set srcintf pevrf_leak_zone
    set dstintf "underlay"
    set srcaddr "all"
    set dstaddr "all"
    set action accept
    set schedule "always"
    set service "ALL"
    set utm-status enable
    set ssl-ssh-profile certificate-inspection
    set application-list "default"
    set logtraffic all
    set nat enable
  next
  edit 4
    set name "Internet (RIA)"
    set srcintf pevrf_leak_zone
    set dstintf "overlay"
    set srcaddr "all"
    set dstaddr "all"
    set action accept
    set schedule "always"
    set service "ALL"
    set utm-status enable
    set ssl-ssh-profile certificate-inspection
    set application-list "default"
    set logtraffic all
    set nat enable
    set ippool enable
    set poolname "Lo"
  next
  edit 5
    set name "Health-Check"
    set srcintf "overlay"
    set dstintf "Lo"
    set srcaddr "all"
    set dstaddr "all"
    set action accept
    set schedule "always"
    set service "PING"
  next
end

